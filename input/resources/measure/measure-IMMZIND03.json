      "valueString": "IMMZIND03"
    }, {
      "url": "name",
      "valueString": "Administrative Gender Stratifier"
    }, {
      "url": "statement",
      "valueString": "/*\n * Disaggregator: Administrative Gender (or Biological Sex)\n */define \"Administrative Gender Stratifier\":\n\tIMMZIndCom.\"By Administrative Gender Stratifier\""
    }, {
      "url": "displaySequence",
      "valueInteger": 1
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "IMMZCommon"
    }, {
      "url": "name",
      "valueString": "Doses Administered to Patient"
    }, {
      "url": "statement",
      "valueString": "/**\n * @dataElement All Doses Administered to Patient to patient ordered newest to oldest\n */define \"Doses Administered to Patient\":\n  [Immunization] I\n    where I.status = 'completed'\n    sort by (occurrence as FHIR.dateTime) desc"
    }, {
      "url": "displaySequence",
      "valueInteger": 2
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "IMMZCommon"
    }, {
      "url": "name",
      "valueString": "HepB Doses Administered to Patient"
    }, {
      "url": "statement",
      "valueString": "/**\n * @dataElement HepB containing Doses Administered to Patient\n */define \"HepB Doses Administered to Patient\":\n  \"Doses Administered to Patient\" I\n  where\n    I.vaccineCode in IMMZc.\"HepB Vaccine\""
    }, {
      "url": "displaySequence",
      "valueInteger": 3
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "IMMZIndicatorCommon"
    }, {
      "url": "name",
      "valueString": "HepB Birth Doses Administered to Patient During Measurement Period"
    }, {
      "url": "statement",
      "valueString": "/*\n * HepB Doses administered to the patient during the measurement period\n */define \"HepB Birth Doses Administered to Patient During Measurement Period\":\n    IMMZCom.\"HepB Doses Administered to Patient\" I\n\t\twhere IMMZCom.ToDate(I.occurrence) during \"Measurement Period\"\n        and (singleton from I.protocolApplied).doseNumber = 0"
    }, {
      "url": "displaySequence",
      "valueInteger": 4
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "IMMZIND03"
    }, {
      "url": "name",
      "valueString": "measure-population"
    }, {
      "url": "statement",
      "valueString": "/*\n * Numerator: Number of administrations of vaccines containing a Hepatitis B component at birth (dose sequence 0)\n * Numerator Computation: COUNT immunization events WHERE administered product is a HepB (IMMZ.A1.DE6) during reporting period and dose sequence = 0\n */define \"measure-population\":\n\texists(IMMZIndCom.\"HepB Birth Doses Administered to Patient During Measurement Period\")"
    }, {
      "url": "displaySequence",
      "valueInteger": 5
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "IMMZIndicatorCommon"
    }, {
      "url": "name",
      "valueString": "By Geographic Region Stratifier"
    }, {
      "url": "statement",
      "valueString": "/**\n * By Administrative Gender of Patient Stratifier\n */define \"By Geographic Region Stratifier\":\n    First(Patient.address A where A.use in { 'home' }).state"
    }, {
      "url": "displaySequence",
      "valueInteger": 6
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "IMMZIND03"
    }, {
      "url": "name",
      "valueString": "Geographic Region Stratifier"
    }, {
      "url": "statement",
      "valueString": "/*\n * Disaggregator: Geographic Region\n */define \"Geographic Region Stratifier\":\n\tIMMZIndCom.\"By Geographic Region Stratifier\""
    }, {
      "url": "displaySequence",
      "valueInteger": 7
    } ]
  } ],
